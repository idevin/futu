{
    "name": "vicklr/materialized-model",
    "description": "Materialized Path for Eloquent model hierarchies",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vicklr",
            "email": "vicklr@vicklr.com",
            "homepage": "https://vicklr.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Vicklr\\MaterializedModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vicklr\\MaterializedModel\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vicklr\\MaterializedModel\\MaterializedModelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
